import os
os.system('cls'if os.name=='nt'else 'clear')

#Se le solicita al usuario su nombre y el sexo y se le indica en que grupo estara.

nombre=input("Compo te llamas?")
genero= input("Cual es tu sexo (M o H)?")
if (genero=="M" and nombre.lower()<'m') or (genero=="H" and nombre.lower()>'n'):
    grupo= "A"
else:
    grupo="B"
print("Tu grupo es " + grupo)
